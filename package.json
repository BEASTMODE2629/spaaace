{
  "name": "spaaace",
  "version": "1.0.0",
  "engines": {
    "node": "4.3.0"
  },
  "description": "An HTML5 multiplayer space shooter based on Incheon",
  "main": "main.js",
  "dependencies": {
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "express": "^4.14.0",
    "howler": "^2.0.2",
    "incheon": "OpherV/Incheon.git#develop",
    "pixi.js": "^4.2.3",
    "query-string": "^4.2.3",
    "socket.io": "^1.4.8",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "eslint": "^3.10.2",
    "eslint-config-google": "^0.7.0"
  },
  "scripts": {
    "start": "node main.js",
    "postinstall": "browserify src/client/clientEntryPoint.js -i nodejs-physijs -o dist/bundle.js ",
    "watchify": "watchify src/client/clientEntryPoint.js -i nodejs-physijs -o dist/bundle.js -v -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpherV/spaaace.git"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "keywords": [
    "game",
    "incheon",
    "multiplayer",
    "websockets"
  ],
  "author": "Opher Vishnia",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OpherV/spaaace/issues"
  },
  "homepage": "https://github.com/OpherV/spaaace#readme"
}
